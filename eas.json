{
  "cli": {
    "version": ">= 3.13.4",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType":  "app-bundle"
      },
      "channel": "production"
     
     
    }
  },
  "submit": {
    "production": {}
  }
}
